{"components/common/subscription-form":"<h3 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h3>\n<p class=\"footer-info-text\">{{lang 'newsletter.subscribe_intro'}}</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\"\n                       id=\"nl_email\"\n                       name=\"nl_email\"\n                       type=\"email\"\n                       value=\"{{customer.email}}\"\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       required\n                >\n                <button class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\">{{lang 'newsletter.subscribe_submit'}}</button>\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n","components/custom/home-bottom-callout":"<section class=\"container home-bottom-callout\">\n    \n    <div class=\"home-bottom-callout__container container\">\n        <!-- giveaway -->\n        <article class=\"home-bottom-card home-bottom-card--giveaway\" >\n            <div class=\"home-bottom-card__content\">\n                <h1 class=\"home-bottom-card__heading h2\">\n                    {{~lang 'custom.home.bottom-callout.callout-1.title'~}}\n                </h1>\n                <p class=\"home-bottom-card__text\">\n                    <span class=\"home-bottom-card__text-month\">\n                        {{lang 'custom.home.bottom-callout.callout-1.month'}}\n                    </span>\n\n                    <br>\n\n                    <span class=\"home-bottom-card__text-winner h3\">\n                        {{lang 'custom.home.bottom-callout.callout-1.winner-name'}}\n                    </span>\n\n                    <br>\n\n                    {{lang 'custom.home.bottom-callout.callout-1.location'}}\n                </p>\n            </div>\n            <div class=\"home-bottom-card__item\">\n                <div class=\"home-bottom-card__item-content\">\n                    <h2 class=\"home-bottom-card__item-heading h4\">\n                        {{~lang 'custom.home.bottom-callout.callout-1.item-month'~}}\n                    </h2>\n                    <div class=\"home-bottom-card__item-wrapper\">\n                        <div class=\"home-bottom-card__item-image\">\n                            <img src=\"{{cdn (concat 'webdav:' (lang 'custom.home.bottom-callout.callout-1.item-image'))}}\"\n                                alt=\"{{lang 'custom.home.bottom-callout.callout-1.item-image-alt'}}\">\n                        </div>\n                        <div class=\"home-bottom-card__item-text\">\n                            <div>\n                                <img src=\"{{cdn (concat 'webdav:' (lang 'custom.home.bottom-callout.callout-1.item-brand-img'))}}\"\n                                    alt=\"{{lang 'custom.home.bottom-callout.callout-1.item-brand-img-alt'}}\">\n                            </div>\n                            <p>\n                                <span>\n                                    {{lang 'custom.home.bottom-callout.callout-1.item-title'}}\n                                </span>\n                                <br>\n                                {{{lang 'custom.home.bottom-callout.callout-1.item-value'}}}\n                            </p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n                <p class=\"home-bottom-card__link\">\n                    {{lang 'custom.home.bottom-callout.callout-1.link-text'}}\n                    <a  href=\"{{lang 'custom.home.bottom-callout.callout-1.link-url'}}\" >\n                        Join today!\n                    </a> \n                </p>\n            </article>\n\n        <!-- VIP Registration -->\n        <article class=\"bg-paint-splatter home-bottom-card home-bottom-card--vip\" >\n            <div class=\"home-bottom-card__content\">\n                <h1 class=\"home-bottom-card__heading h2\">\n                    {{~lang 'custom.home.bottom-callout.callout-2.title'~}}\n                </h1>\n                <ul class=\"home-bottom-card__list\">\n                    <li class=\"h3\">{{{lang 'custom.home.bottom-callout.callout-2.bullet-1'}}}</li>\n                    <li class=\"h3\">{{{lang 'custom.home.bottom-callout.callout-2.bullet-2'}}}</li>\n                    <li class=\"h3\">{{{lang 'custom.home.bottom-callout.callout-2.bullet-3'}}}</li>\n                </ul>\n            </div>\n            <div class=\"home-bottom-card__newsletter\">\n                {{#if settings.show_newsletter_box}}\n                {{> components/common/subscription-form}}\n                {{/if}}\n            </div>\n                <p class=\"home-bottom-card__link\">\n                    {{{lang 'custom.home.bottom-callout.callout-2.details-text'}}}\n                </p>\n            </article>\n\n        <!-- Blog -->\n        <a class=\"home-bottom-card home-bottom-card--blog\" href=\"{{lang 'custom.home.bottom-callout.callout-3.button-link'}}\">\n            <article >\n                <h1 class=\"home-bottom-card__heading h2\">{{lang 'custom.home.bottom-callout.callout-3.title'}}</h1>\n                <img class=\"home-bottom-card__image\"\n                    src=\"{{cdn (concat 'webdav:' (lang 'custom.home.bottom-callout.callout-3.image'))}}\"\n                    alt=\"{{lang 'custom.home.bottom-callout.callout-3.image-alt'}}\">\n                <div class=\"home-bottom-card__content\">\n                    <div class=\"home-bottom-card__icon-wrapper\">\n                        <span class=\"icon icon--youtube\">\n                            <svg>\n                                <use xlink:href=\"#icon-youtube\"></use>\n                            </svg>\n                        </span>\n                    </div>\n                    <div class=\"home-bottom-card__text\">\n                        <h2 class=\"home-bottom-card__text-heading h3\">{{lang\n                            'custom.home.bottom-callout.callout-3.heading'}}</h2>\n                        <p class=\"home-bottom-card__text-desc\">{{lang 'custom.home.bottom-callout.callout-3.text'}}</p>\n\n                    </div>\n                </div>\n            </article>\n        </a>\n\n    </div>\n</section>\n"}
