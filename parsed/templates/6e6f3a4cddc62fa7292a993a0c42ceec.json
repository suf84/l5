{"components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu {{#if hover_menu}}navPages-action--toplevel{{/if}}{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n   {{#if hover_menu}}data-collapsible-disabled-breakpoint=\"medium\"{{/if}}\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <div class=\"navPage-subMenu-container\">\n        <ul class=\"navPage-subMenu-list\">\n            <li class=\"navPage-subMenu-item\">\n                <a class=\"navPage-subMenu-action navPages-action\"\n                   href=\"{{url}}\"\n                   aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n                >\n                    {{lang 'category.view_all.name' category=name}}\n                </a>\n            </li>\n            {{#each children}}\n                <li class=\"navPage-subMenu-item\">\n                    {{#if children}}\n                        <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                           href=\"{{url}}\"\n                           aria-label=\"{{name}}\"\n                        >\n                            {{name}}\n                            <span class=\"collapsible-icon-wrapper\"\n                                data-collapsible=\"navPages-{{id}}\"\n                                data-collapsible-disabled-breakpoint=\"medium\"\n                                data-collapsible-disabled-state=\"open\"\n                                data-collapsible-enabled-state=\"closed\"\n                            >\n                                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                    <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n                                </i>\n                            </span>\n                        </a>\n                        <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                            {{#each children}}\n                            <li class=\"navPage-childList-item\">\n                                <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                                   href=\"{{url}}\"\n                                   aria-label=\"{{name}}\"\n                                >\n                                    {{name}}\n                                </a>\n                            </li>\n                            {{/each}}\n                        </ul>\n                    {{else}}\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                           href=\"{{url}}\"\n                           aria-label=\"{{name}}\"\n                        >\n                            {{name}}\n                        </a>\n                    {{/if}}\n                </li>\n            {{/each}}\n            {{!#if (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.view')) '===' 'true'}}\n                <!-- <li class=\"navPage-subMenu-item navPage-subMenu-item--mega-menu\">\n                    {{> components/custom/mega-menu }}\n                </li> -->\n            {{!/if}}\n        </ul>\n        {{#if (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.view')) '===' 'true'}}\n            <div class=\"navPage-subMenu-item navPage-subMenu-item--mega-menu\">\n                {{> components/custom/mega-menu }}\n            </div>\n        {{/if}}\n    </div>\n\n    <div class=\"navPage-subMenu-list--bottomContainer\">\n        <div class=\"navPage-subMenu-list--textContain-1\">   \n            <a href=\"/shipping-returns/\">FREE SHIPPING ON ORDERS OVER $50*</a>\n        </div>\n        <div class=\"navPage-subMenu-list--textContain-2\">\n            <a href=\"{{url}}\">VIEW ALL TOOL SETS</a>\n        </div>\n    </div>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n","components/common/navigation-list-alternate":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       data-collapsible=\"navPages-{{id}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n","components/common/navigation-menu":"<nav class=\"navPages\">\n    <div class=\"navPages-quickSearch\">\n        {{> components/common/quick-search name='nav-menu-quick-search'}}\n    </div>\n    <ul class=\"navPages-list{{#if ../theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\n        {{#each categories}}\n        <li class=\"navPages-item\">\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                {{> components/common/navigation-list hover_menu=../theme_settings.its.toggle_hover_menu}}\n            {{/if}}\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                {{> components/common/navigation-list-alternate}}\n            {{/if}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                 <li class=\"navPages-item navPages-item-page\">\n                     <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\"\n                        href=\"{{url}}\"\n                        aria-label=\"{{name}}\"\n                     >\n                         {{name}}\n                     </a>\n                 </li>\n             {{/each}}\n        {{/unless}}\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\"\n                   href=\"#\"\n                   data-collapsible=\"navPages-currency\"\n                   aria-controls=\"navPages-currency\"\n                   aria-expanded=\"false\"\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\n                >\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}}\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use>\n                        </svg>\n                    </i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{{switch_url}}}\"\n                               aria-label=\"{{name}}\"\n                               data-currency-code=\"{{{code}}}\"\n                               {{#if cart_currency_switch_url.length '>' 0 }}\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                               {{/if}}\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                            >\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\"\n                   href=\"{{urls.compare}}\"\n                   data-compare-nav\n                   aria-label=\"{{lang 'common.compare'}}\"\n                >\n                    {{lang 'common.compare'}}\n                    <span class=\"countPill countPill--positive countPill--alt\"></span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\"\n                   href=\"{{urls.account.index}}\"\n                   data-collapsible=\"navPages-account\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n                    </i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.orders.all}}\"\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\n                            >\n                                {{lang 'account.nav.orders'}}\n                            </a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.returns}}\"\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\n                                >\n                                    {{lang 'account.nav.returns'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.inbox}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\n                            >\n                                {{lang 'account.mobile_nav.messages'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.addresses}}\"\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\n                            >\n                                {{lang 'account.nav.addresses'}}\n                            </a>\n                        </li>\n                        {{#if settings.show_payment_methods}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.payment_methods.all}}\"\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\n                                >\n                                    {{lang 'account.nav.payment_methods'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.wishlists.all}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\n                            >\n                                {{lang 'account.mobile_nav.wishlists'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.recent_items}}\"\n                               aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\n                            >\n                                {{lang 'account.nav.recently_viewed'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.details}}\"\n                               aria-label=\"{{lang 'account.nav.settings'}}\"\n                            >\n                                {{lang 'account.nav.settings'}}\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.auth.logout}}\"\n                   aria-label=\"{{lang 'common.logout'}}\"\n                >\n                    {{lang 'common.logout'}}\n                </a>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n\n                {{#if theme_settings.its.popup_login}}\n                    <a class=\"navPages-action\"\n                        aria-label=\"{{lang 'common.login'}}\"\n                        href=\"##\"\n                        data-reveal-id=\"popupLoginWindow\"\n                    >\n                        {{lang 'common.login'}}\n                    </a>\n                {{else}}\n                    <a class=\"navPages-action\"\n                    href=\"{{urls.auth.login}}\"\n                    aria-label=\"{{lang 'common.login'}}\"\n                    >\n                        {{lang 'common.login'}}\n                    </a>\n                {{/if}}\n\n                {{#if settings.account_creation_enabled}}\n                    {{lang 'common.or'}}\n                    <a class=\"navPages-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/if}}\n        {{!-- {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navPages-item\">\n                {{> components/common/social-links}}\n            </li>\n        {{/if}} --}}\n    </ul>\n</nav>\n","components/common/quick-search":"    <form class=\"form header-1__form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       autocomplete=\"off\"\n                >\n                <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-search\" /></svg></i>\n            </div>\n        </fieldset>\n    </form>\n    {{!-- <section class=\"quickSearchResults\" data-bind=\"html: results\"></section> --}}\n    {{!-- <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p> --}}\n","components/common/social-links":"{{#if footer}}\n    {{#if social_media}}\n        <ul class=\"socialLinks socialLinks--alt\">\n            {{#each social_media}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"img--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <img class=\"lazyload\" data-src=\"{{cdn (concat (concat '/img/' display_name) '-footer.png')}}\" src=\"{{cdn 'img/loading.svg'}}\">\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    {{/if}}\n{{else}}\n    {{#if social_media}}\n        <ul class=\"socialLinks socialLinks--alt\">\n            {{#each social_media}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    {{/if}}\n{{/if}}\n","components/custom/mega-menu":"<aside class=\"mega-menu\">\n    <a href=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.link')}}\" class=\"mega-menu__link\">\n        <figure class=\"mega-menu__image-container\">\n            {{#if (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image')) '===' 'webdav:/img/mega-menu/'}}\n                <img class=\"lazyload mega-menu__image\" data-src=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image-placeholder')}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image-alt-text')}}\">\n            {{else}}\n                <img class=\"lazyload mega-menu__image\" data-src=\"{{cdn (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image'))}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image-alt-text')}}\">\n            {{/if}}\n        </figure>\n        <section class=\"mega-menu__text-container\">\n            <h3 class=\"mega-menu__title\">{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.title')}}</h3>\n            <p class=\"mega-menu__text\">{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.text')}}<span class=\"mega-menu__text-discount\">{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.discount')}}</span></p>\n        </section>\n    </a>\n</aside>\n"}
