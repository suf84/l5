{"components/custom/category-view-toggle":"{{#if theme_settings.its.category_view_toggle}}\r\n{{inject 'defaultViewType' theme_settings.product_list_display_mode}}\r\n    <div class=\"category__view-toggle\">\r\n        <button aria-label=\"Toggle Product List View\" class=\"category__toggle-view js-category__toggle-view {{#if type 'list'}}active-category-view{{/if}}\" data-view-type=\"list\">\r\n                <span class=\"icon icon--list\">\r\n                <svg><use xlink:href=\"#icon-list\"></use></svg>\r\n            </span>\r\n        </button>\r\n        <button aria-label=\"Toggle Product Grid View\" class=\"category__toggle-view js-category__toggle-view {{#if type 'grid'}}active-category-view{{/if}}\" data-view-type=\"grid\">\r\n                <span class=\"icon icon--th\">\r\n                <svg><use xlink:href=\"#icon-th\"></use></svg>\r\n            </span>\r\n        </button>\r\n    </div>\r\n{{/if}}\n"}
