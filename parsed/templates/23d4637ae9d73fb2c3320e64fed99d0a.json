{"components/custom/mega-menu":"<aside class=\"mega-menu\">\n    <a href=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.link')}}\" class=\"mega-menu__link\">\n        <figure class=\"mega-menu__image-container\">\n            {{#if (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image')) '===' 'webdav:/img/mega-menu/'}}\n                <img class=\"lazyload mega-menu__image\" data-src=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image-placeholder')}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image-alt-text')}}\">\n            {{else}}\n                <img class=\"lazyload mega-menu__image\" data-src=\"{{cdn (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image'))}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.image-alt-text')}}\">\n            {{/if}}\n        </figure>\n        <section class=\"mega-menu__text-container\">\n            <h3 class=\"mega-menu__title\">{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.title')}}</h3>\n            <p class=\"mega-menu__text\">{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.text')}}<span class=\"mega-menu__text-discount\">{{lang (concat (concat 'custom.header.mega-menu.category-'@index)'.discount')}}</span></p>\n        </section>\n    </a>\n</aside>\n"}
