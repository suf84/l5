{"components/custom/category-cta":"<section class=\"container category-cta\">\n    <h2 class=\"category-cta-heading h1\">\n        {{lang 'custom.home.category-cta.heading'}}\n    </h2>\n    <div class=\"category-cta__container\">\n        {{#for 1 (lang 'custom.home.category-cta.number-of-ctas')}}\n            <article class=\"category-card\">\n                <figure class=\"category-card__figure\">\n                    <img src=\"{{cdn (concat 'webdav:' (lang (concat (concat 'custom.home.category-cta.cta-'$index)'.image')))}}\" alt=\"{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.image-alt')}}\">\n                </figure>\n                <div class=\"category-card__content\">\n                    <div class=\"category-card__content-container\">\n                        <h1 class=\"category-card__heading h2\">\n                            {{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.title')}}\n                        </h1>\n                        <p class=\"category-card__text\">{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.text')}}</p>\n                    </div>\n                    <a href=\"{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.button-link')}}\" class=\"button button--primary category-card__button\">{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.button-text')}}</a>\n                </div>\n            </article>\n        {{/for}}\n    </div>\n</section>\n"}
