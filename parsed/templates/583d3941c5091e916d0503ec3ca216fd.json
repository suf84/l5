{"components/products/description-tabs":"<div class=\"tabs-wrapper\">\n    <ul class=\"container tabs\" data-tab>\n        {{#if product.description}}\n            <li class=\"tab is-active\">\n                <a class=\"tab-title\" href=\"#tab-description\">{{lang 'products.description'}}</a>\n            </li>\n        {{/if}}\n        {{#if product.warranty}}\n            <li class=\"tab {{#unless product.description}}is-active{{/unless}} tab--warranty\">\n                <a class=\"tab-title\" href=\"#tab-warranty\">{{lang 'products.warranty'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</div>\n\n<div class=\"tabs-contents\">\n    {{#if product.description}}\n        <div class=\"tab-content tab-content--description is-active\" id=\"tab-description\">\n            <div class=\"container\">\n                {{{product.description}}}\n            </div>\n        </div>\n    {{/if}}\n\n    {{#if product.warranty}}\n        <div class=\"container tab-content tab-content--warranty {{#unless product.description}}is-active{{/unless}}\" id=\"tab-warranty\">\n            {{{product.warranty}}}\n        </div>\n    {{/if}}\n</div>\n"}
