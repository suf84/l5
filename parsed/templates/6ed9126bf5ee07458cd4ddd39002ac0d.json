{"components/common/alert/alert-error":"<div class=\"alertBox alertBox--error\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span id=\"alertBox-message-text\">{{{this}}}</span>\n    </p>\n</div>\n","components/common/alert/alert-success":"<div class=\"alertBox alertBox--success\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span id=\"alertBox-message-text\">{{{this}}}</span>\n    </p>\n</div>\n","components/common/modal/modal-close-btn":"<button class=\"modal-close\"\n        type=\"button\"\n        title=\"{{lang 'common.close'}}\"\n        {{additional_attr}}\n>\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\n    <span aria-hidden=\"true\">&#215;</span>\n</button>\n","components/custom/popup-login-window-modal":"<div id=\"popupLoginWindow\" class=\"modal modal--small modal--popupLoginWindow\" data-reveal>\n\t<div class=\"modal-header\">\n\t\t<h2 class=\"modal-header-title\">{{lang 'common.login'}}</h2>\n\t\t{{> components/common/modal/modal-close-btn }}\n\t</div>\n\t<div class=\"modal-body\">\n\t\t{{> components/common/alert/alert-error (lang 'custom.other.popup-login.error')}}\n\t\t{{> components/common/alert/alert-success (lang 'custom.other.popup-login.success')}}\n\t\t<form class=\"form\" id=\"popupLoginWindowForm\">\n\t\t\t<div class=\"form-field\">\n\t\t\t\t<label class=\"form-label\" for=\"login_email\">{{lang 'login.field_email'}}</label>\n\t\t\t\t<input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\">\n\t\t\t</div>\n\t\t\t<div class=\"form-field\">\n\t\t\t\t<label class=\"form-label\" for=\"login_pass\">{{lang 'login.field_password'}}</label>\n\t\t\t\t<input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\n\t\t\t</div>\n\t\t\t<div class=\"form-actions\">\n\t\t\t\t<input type=\"submit\" class=\"button button--primary\" value=\"{{lang 'login.submit_value' }}\">\n\t\t\t\t<a class=\"button button--secondary\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n\t\t\t\t<a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\n\t\t\t</div>\n\t\t</form>\n\t</div>\n\t<div class=\"loadingOverlay\" style=\"display: none;\"></div>\n</div>\n"}
